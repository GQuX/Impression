<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
	<!-- Main Color Pallette -->
	<Color x:Key="Color1">#5C4E45</Color>
	<SolidColorBrush x:Key="ColorBrush1" Color="{StaticResource Color1}"/>
	
	<Color x:Key="Color2">#9E9085</Color>
	<SolidColorBrush x:Key="ColorBrush2" Color="{StaticResource Color2}"/>
	
	<Color x:Key="Color3">#E9CBB3</Color>
	<SolidColorBrush x:Key="ColorBrush3" Color="{StaticResource Color3}"/>
	
	<Color x:Key="Color4">#DFDCED</Color>
	<SolidColorBrush x:Key="ColorBrush4" Color="{StaticResource Color4}"/>
	
	<Color x:Key="Color5">#9284B5</Color>
	<SolidColorBrush x:Key="ColorBrush5" Color="{StaticResource Color5}"/>
	
	<!-- Background Gradient -->
	<LinearGradientBrush x:Key="BackgroundGradient" StartPoint="1 0">
		<GradientStop Offset="0.4" Color="{StaticResource Color1}"/>
		<GradientStop Offset="1.0" Color="{StaticResource Color2}"/>
	</LinearGradientBrush>
	

</ResourceDictionary>